pipeline {
    agent any

    environment {
        BUCKET = "jenking-demo"
    }

    stages {
        stage('Deploy to S3') {
            steps {
                script {
                    bat "C:\\Users\\nico\\demo-jenkins\\aws s3 sync . s3://\$BUCKET --exclude '.git/*'"
                    bat "C:\\Users\\nico\\demo-jenkins\\aws s3 ls s3://\$BUCKET"
                }
            }
        }
    }
}



